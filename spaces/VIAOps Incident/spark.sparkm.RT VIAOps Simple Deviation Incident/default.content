          <Imports><![CDATA[object IncidentMsgUtils {

  def replaceVars(template: String, vs: Tuple2[String, Any]*): String = {
    var replaced = template
    vs.foreach(x => {
      val key = "%{" + x._1 + "}"
      val v = x._2 match {
        case null => ""
        case None => ""
        case Some(x) => x.toString
        case _ => x._2.toString()
      }
      replaced = replaced.replace(key, v)
    })
    replaced
  }
}

