:"CustomCode","desc":"Custom code {scala_editor}","bindings":[{"field":"scala_editor","type":"scala_editor","editorType":"scripteditor","literalValue":"CODE","value":"\r\n    val kpis = ctx.getGlobalVar[KPIs](\"KPIs\").getOrElse(KPIs(Seq())).kpis.sortBy(_.start_tm)\r\n    val ctxs = ctx.getGlobalVar[CTXs](\"CTXs\").getOrElse(CTXs(Seq())).ctxs.sortBy(_.contextual_metric_start_tm)\r\n    val ctx_skew_time = ctx.getGlobalVar[Long](\"ctx_skew_time\").get\r\n    import viaops.incident.EnrichContextualMetric.AnomalyWithCMID\r\n    import viaops.incident.EnrichContextualMetric.ContextualAnomaly\r\n    import viaops.incident_score_anomaly\r\n    def map_result(kpi: AnomalyWithCMID, ctx: Option[ContextualAnomaly]): incident_score_anomaly = {\r\n      incident_score_anomaly(\r\n        kpi.high_metric_deviation,\r\n        kpi.med_metric_deviation,\r\n        kpi.metric_base_metric_id,\r\n        kpi.dim_keys,\r\n        kpi.metric_dim_values,\r\n        kpi.anomaly_interval,\r\n        kpi.start_tm,\r\n        kpi.end_tm,\r\n        kpi.metric_id,\r\n        kpi.metric_measure,\r\n        kpi.metric_baseline,\r\n        kpi.median_baseline,\r\n        kpi.mean_baseline,\r\n        kpi.date_key,\r\n        kpi.config_metric_id,\r\n        kpi.threshold_policy,\r\n        kpi.algorithm,\r\n        kpi.emit_notification,\r\n        kpi.threshold_high,\r\n        kpi.threshold_low,\r\n        kpi.high_threshold_period,\r\n        kpi.expire_after,\r\n        kpi.cool_down_period,\r\n        kpi.warmup_period,\r\n        kpi.warmup_threshold,\r\n        kpi.min_metric_vol,\r\n        kpi.causation_measure,\r\n        kpi.causation_top_count,\r\n        kpi.causation_min_measure,\r\n        kpi.causation_rule_name,\r\n        kpi.upd_dt,\r\n        kpi.config_dim_pattern,\r\n        kpi.service_name,\r\n        kpi.sub_service_name,\r\n        kpi.correlation_time,\r\n        kpi.groupable,\r\n        kpi.config_base_metric_id,\r\n        kpi.metric_hist_per,\r\n        kpi.max_low_per,\r\n        kpi.max_med_per,\r\n        kpi.tot_max_low_per,\r\n        kpi.tot_max_med_per,\r\n        kpi.med_threshold_period,\r\n        kpi.mode,\r\n        kpi.min_metric_deviation,\r\n        kpi.max_contextual_metric_time_skew,\r\n        kpi.min_contextual_metric_vol,\r\n        kpi.min_contextual_metric_deviation,\r\n        kpi.med_contextual_metric_deviation,\r\n        kpi.high_contextual_metric_deviation,\r\n        kpi.window,\r\n        kpi.threshold_med,\r\n        kpi.causation_suppl_measure,\r\n        kpi.min_factor,\r\n        kpi.med_factor,\r\n        kpi.score_method,\r\n        kpi.max_warmup_period,\r\n        kpi.late_data_timer,\r\n        kpi.kpi_metric_id,\r\n        kpi.kpi_base_metric_id,\r\n        kpi.kpi_dim_keys,\r\n        kpi.kpi_dim_pattern,\r\n        kpi.contextual_metric_id,\r\n        ctx.map(_.contextual_metric_start_tm),\r\n        ctx.map(_.contextual_metric_end_tm),\r\n        ctx.map(_.contextual_metric_aggr),\r\n        ctx.flatMap(_.contextual_metric_baseline),\r\n        ctx.flatMap(_.contextual_metric_median_baseline),\r\n        ctx.flatMap(_.contextual_metric_mean_baseline))\r\n    }\r\n\r\n    if (kpis.isEmpty) {\r\n      if (!ctxs.isEmpty) {\r\n        val min_ctx_tm = ctxs.map(_.contextual_metric_start_tm).max - ctx_skew_time\r\n        ctx.setGlobalVar(\"CTXs\", CTXs(ctxs.filter(_.contextual_metric_start_tm &gt;= min_ctx_tm)))\r\n      }\r\n    } else {\r\n      if (ctxs.isEmpty) {\r\n        kpis.foreach(kpi =&gt; {\r\n          ctx.emitOutput(\"kpi\", map_result(kpi, None))\r\n        })\r\n      } else {\r\n        var search_ctx_ind = 0\r\n        kpis.foreach(kpi =&gt; {\r\n          var end_match = false\r\n          var match_ctx: Option[ContextualAnomaly] = None\r\n          var min_skew = Long.MaxValue\r\n          var last_skew = Long.MaxValue\r\n          var ctx_ind = search_ctx_ind\r\n          println(\"====\" + kpi.metric_id + \"$$$\" + kpi.start_tm + \"###\" + match_ctx.map(_.contextual_metric_start_tm) + \"---\" + ctx_skew_time)\r\n          while (!end_match &amp;&amp; ctx_ind &lt; ctxs.length) {\r\n            if (search_ctx_ind == ctxs.length) {\r\n              println(\"====end1\")\r\n              end_match = true\r\n            } else {\r\n              val skew = Math.abs(ctxs(ctx_ind).contextual_metric_start_tm - kpi.start_tm)\r\n              println(\"====\" + ctxs(ctx_ind).contextual_metric_start_tm + \"@@@\" + skew)\r\n              if (skew == 0) {\r\n                match_ctx = Option(ctxs(ctx_ind))\r\n                println(\"====end2\")\r\n                end_match = true\r\n              } else if (skew &gt; last_skew) {\r\n                println(\"====end3\")\r\n                end_match = true\r\n              } else if (skew &lt;= ctx_skew_time) {\r\n                println(\"====assign ctx\")\r\n                match_ctx = Option(ctxs(ctx_ind))\r\n              } else {\r\n                search_ctx_ind = ctx_ind\r\n              }\r\n              last_skew = skew\r\n              ctx_ind = ctx_ind + 1\r\n            }\r\n          }\r\n          ctx.emitOutput(\"kpi\", map_result(kpi, match_ctx))\r\n        })\r\n        val g_kpis = kpis.groupBy(kpi =&gt; kpi.metric_id)\r\n        val min_ctx_tm = g_kpis.mapValues(kpi =&gt; kpi.map(_.start_tm).max).values.min - ctx_skew_time\r\n        ctx.setGlobalVar(\"CTXs\", CTXs(ctxs.filter(_.contextual_metric_start_tm &gt;= min_ctx_tm)))\r\n      }\r\n\r\n    }\r\n    ctx.setGlobalVar(\"KPIs\", KPIs(Seq()))\r\n  "}]}]}}
</Properties>
