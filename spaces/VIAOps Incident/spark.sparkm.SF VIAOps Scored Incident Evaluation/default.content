          <Imports><![CDATA[object IncidentMsgUtils {

    def replaceVars(template: String, vs: Tuple2[String, Any]*): String = {
      var ret = template
      var posBegin = ret.indexOf("{")
      var posEnd = ret.indexOf("}", posBegin + 1)
      while (posBegin > 0 && posEnd > posBegin) {
        val key = ret.substring(posBegin+1, posEnd)
        vs.filter(_._1.equals(key)).foreach(x => {
          val v = x._2 match {
            case null => ""
            case None => ""
            case Some(x) => x.toString
            case _ => x._2.toString()
          }
          ret = ret.replace("{" + key + "}", v)
        })
        posBegin = ret.indexOf("{")
        posEnd = ret.indexOf("}", posBegin + 1)
      }
      ret
    }
}
