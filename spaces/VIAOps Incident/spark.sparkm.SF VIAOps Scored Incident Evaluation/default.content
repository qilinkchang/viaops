                <Variable alias="contextual_metric_time_skew_rule" outputType="boolean" value="if (contextual_metric_id != &apos;none&apos; and abs(contextual_metric_time_skew) &lt; to_nonoption(max_contextual_metric_time_skew),true,false)"/>
                <Variable alias="score" outputType="double" value="(to_option(metric_measure)-to_option(metric_baseline))/to_option(adj_factor) * 100"/>
                <Variable alias="active_metric_rule" outputType="boolean" value="if(metric_measure &gt;= min_metric_vol and m_deviation &gt;= min_metric_deviation,true,false)"/>
                <Variable alias="active_contextual_metric_rule" outputType="boolean" value="if(contextual_metric_time_skew_rule and contextual_metric_aggr &gt;= min_contextual_metric_vol and c_deviation &gt;= min_contextual_metric_deviation,true,false)"/>
